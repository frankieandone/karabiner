    {
      "description": "Map modifier keys.",
      "manipulators": [
        {
          "from": {
            "key_code": "home",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "page_up",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
