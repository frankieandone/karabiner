{
  "title": "frankieandone's keymap",
  "rules": [
    {
      "description": "Map fn+{arrow keys} => media keys i.e. fn+up/down => vol up/down, fn+left/right => media prev/next",
      "manipulators": [
        {
          "from": {
            "key_code": "left_arrow",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "rewind"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "right_arrow",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "fast_forward"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "up_arrow",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "volume_increment"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "down_arrow",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "volume_decrement"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map number row incl. equal and hyphen keys to their respective f-numbers when pressing fn key+{number} => {f-number}, fn+equal => f11 and fn+hyphen => f12",
      "manipulators": [
